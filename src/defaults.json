      {
        "name": "Satellite",
        "tile": "https://cyberjapandata.gsi.go.jp/xyz/seamlessphoto/{z}/{x}/{y}.jpg",
        "attribution": "GSI Japan",
        "attribution_url": "http://maps.gsi.go.jp/development/ichiran.html"
      }
    ]
  }
}
